{
  "name": "carrot-css",
  "version": "0.2.6",
  "description": "A simple CSS starter, that assume as less as possible",
  "main": "./dist/carrot.css",
  "style": "./dist/carrot.css",
  "author": "Tom Quinonero <tomquinonero@hey.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomquinonero/carrot-css/"
  },
  "scripts": {
    "getColors": "node cli.js colors",
    "build": "node cli.js colors & node cli.js compile",
    "docs": "node cli.js cssToData & yarn eleventy --serve",
    "docsBuild": "node cli.js cssToData & env NODE_ENV=production yarn eleventy --pathprefix 'carrot-css'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.2.4",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "css": "^3.0.0",
    "eleventy-plugin-sass": "^1.2.0",
    "figlet": "^1.5.0",
    "filesize": "^6.1.0",
    "gh-pages": "^3.1.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.1.0",
    "minimist": "^1.2.5",
    "modern-css-reset": "^1.4.0",
    "moment": "^2.29.1",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8",
    "tailwindcss": "^2.0.3"
  }
}
